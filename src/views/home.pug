extends layouts/main-layout.pug

block title
  title #{username}'s PlaylistApp

block content
  h1(class="p-2 bg-slate-900 text-white") PlaylistApp
  p Some text for you

  each friend in friends
    div(class="bg-blue-900")
      p(class="text-white") #{friend}
  
  article
    p #{pet.type}
    p #{pet.age}